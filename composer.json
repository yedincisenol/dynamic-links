{
    "name": "marcoandreac/dynamic-links",
    "description": "Firebase Dynamic Links for Php and Laravel",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "ext-curl": "*",
        "php" : ">=7.0"
    },
    "keywords": [
      "firebase",
      "laravel",
      "dynamic-links",
      "firebase dynamic-links",
      "php firebase",
      "php"
    ],
    "version": "1.2.6",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibrahim S. Orencik",
            "email": "o@yedincisenol.com"
        }
    ],
    "autoload": {
      "psr-0" : {
        "yedincisenol\\DynamicLinks\\" : "src"
      },
      "psr-4": {
        "yedincisenol\\DynamicLinks\\" : "src"
      }
    },
    "require-dev": {
        "phpunit/phpunit": "6.2"
    },
    "extra": {
      "laravel": {
        "providers": [
          "yedincisenol\\DynamicLinks\\LaravelServiceProvider"
        ]
      }
    }
}
